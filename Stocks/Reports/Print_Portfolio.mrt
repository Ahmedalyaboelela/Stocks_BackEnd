        <Name>PortfolioOpeningStocks</Name>
        <NameInSource>MS SQL</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_PortfolioID,_x0031_,8,0</value>
        </Parameters>
        <SqlCommand>SELECT [pos].[OpeningStocksCount]
	,[pos].[OpeningStockValue]
	,[p].NameAR
	FROM [PortfolioOpeningStocks]as pos,[Partners] as p
	where pos.PartnerID=p.PartnerID and 
	pos.PortfolioID=@PortfolioID</SqlCommand>
      </PortfolioOpeningStocks>
