        <Name>Portfolio</Name>
        <NameInSource>MS SQL</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_PortfolioID,_x0031_,8,0</value>
        </Parameters>
        <SqlCommand>SELECT [p].[Code]
	,[p].[NameAR]
	,[p].[NameEN]
	,[p].[EstablishDate]
	,[p].[Description]
	,[p].[TotalStocksCount]
	,[a].[NameAR] as AccountNameAr
	,[a].[Debit] - [a].[Credit] as totalryal
	FROM Portfolios as p ,PortfolioAccounts as pa,Accounts as a
	where p.PortfolioID = pa.PortfolioID and pa.AccountID=a.AccountID
	and p.PortfolioID=@PortfolioID</SqlCommand>
      </Portfolio>
